<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CONTROLATEC.MainPage"
             Title="Agregar diario">

    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="BtnGuardar" Text="Guardar" Clicked="BtnGuardar_Clicked"/>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="id_cuenta" TextColor="Black"/>
            <Entry Placeholder="" x:Name="id_cuenta"/>
            <Label Text="Fecha" TextColor="Black"/>
            <Entry Placeholder="" x:Name="fecha"/>
            <Label Text="Etiqueta" TextColor="Black"/>
            <Entry Placeholder="" x:Name="etiqueta"/>
            <Label Text="Total" TextColor="Black"/>
            <Entry Placeholder="" x:Name="total"/>
            <Label Text="Estatus" TextColor="Black"/>
            <Entry Placeholder="" x:Name="estatus"/>
        </StackLayout>
    </ContentPage.Content>
    
    </ContentPage>
